{
    "C_Cpp.intelliSenseEngine": "disabled",
    "[python]": {
        "editor.formatOnType": true
    },
    "cmake.configureOnOpen": true,
    "code-runner.clearPreviousOutput": true,
    "code-runner.executorMap": {
        "c": "gcc $fileName -o $fileNameWithoutExt && ./$fileNameWithoutExt",
        "cpp": "/opt/homebrew/bin/aarch64-apple-darwin22-g++-12 $fileName -O2 -W -std=gnu++17 -o ./$fileNameWithoutExt && ./$fileNameWithoutExt",
        "python": "conda activate vscode && python $fileName"
    },
    "code-runner.fileDirectoryAsCwd": true,
    "code-runner.ignoreSelection": true,
    "code-runner.preserveFocus": false,
    "code-runner.runInTerminal": true,
    "code-runner.saveFileBeforeRun": true,
    "code-runner.temporaryFileName": "",
    "debug.console.fontFamily": "JetBrains Mono, MesloLGS Nerd Font, MesloLGS NF, 等距更纱黑体 SC",
    "debug.console.fontSize": 12.6,
    // "editor.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": "on",
    // "editor.cursorBlinking": "smooth",
    "editor.codeLensFontFamily": "JetBrains Mono, MesloLGS Nerd Font, MesloLGS NF, 等距更纱黑体 SC",
    "editor.copyWithSyntaxHighlighting": false,
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "editor.fontFamily": "JetBrains Mono, MesloLGS Nerd Font, MesloLGS NF, 等距更纱黑体 SC",
    "editor.fontSize": 12.6,
    "editor.guides.indentation": false,
    "editor.lineNumbers": "relative",
    "editor.scrollBeyondLastLine": false,
    "editor.suggestSelection": "first",
    "editor.wordBasedSuggestionsMode": "currentDocument",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1,
        "vscodevim.vim": 1,
        "llvm-vs-code-extensions.vscode-clangd": 1
    },
    "git.autoRepositoryDetection": false,
    "git.autorefresh": false,
    "git.decorations.enabled": false,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.variableTypes": true,
    "python.condaPath": "/Users/season/opt/miniconda3/etc/conda",
    "python.defaultInterpreterPath": "/opt/homebrew/Caskroom/miniconda/base/envs/vscode/bin/python",
    "python.diagnostics.sourceMapsEnabled": true,
    "python.languageServer": "Pylance",
    "search.useGlobalIgnoreFiles": true,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.explorerKind": "external",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "terminal.integrated.fontFamily": "JetBrains Mono, MesloLGS Nerd Font, MesloLGS NF, 等距更纱黑体 SC",
    "terminal.integrated.fontSize": 12.6,
    "terminal.integrated.gpuAcceleration": "off",
    "terminal.integrated.inheritEnv": false,
    "vscode-neovim.logOutputToConsole": true,
    "vscode-neovim.mouseSelectionStartVisualMode": true,
    "vscode-neovim.neovimInitVimPaths.darwin": "/Users/season/.config/nvim/init.vim",
    "vscode-neovim.neovimWidth": 1500,
    "window.zoomLevel": 1,
    "workbench.colorTheme": "One Dark Pro Flat",
    "workbench.editor.enablePreview": true,
    "workbench.iconTheme": "material-icon-theme",
    "json.schemaDownload.enable": false,
}